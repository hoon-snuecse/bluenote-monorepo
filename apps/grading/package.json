{
  "name": "@bluenote/grading",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "postinstall": "prisma generate",
    "dev": "next dev",
    "build": "rm -rf node_modules/.prisma && prisma generate && next build",
    "build:analyze": "ANALYZE=true prisma generate && next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "test": "echo 'No tests configured'",
    "db:push": "prisma db push",
    "db:migrate": "tsx scripts/migrate-data.ts",
    "db:seed": "tsx scripts/seed-admin.ts",
    "db:migrate:prod": "prisma migrate deploy",
    "db:migrate:dev": "prisma migrate dev",
    "db:studio": "prisma studio",
    "deploy": "./scripts/deploy.sh",
    "docker:build": "docker build -t grading-app:latest .",
    "docker:run": "docker run -p 3000:3000 --env-file .env.production grading-app:latest",
    "docker:compose": "docker-compose up -d",
    "docker:compose:down": "docker-compose down",
    "docker:compose:logs": "docker-compose logs -f"
  },
