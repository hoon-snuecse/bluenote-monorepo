{
  "name": "bluenote-monorepo",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,md,json}\"",
    "clean": "turbo run clean && rm -rf node_modules"
  },
  "packageManager": "pnpm@9.13.2",
  "devDependencies": {
    "turbo": "^2.3.3",
    "prettier": "^3.3.3",
    "eslint": "^8.57.0",
    "typescript": "^5.5.4",
    "@types/node": "^20.14.0"
  },
  "packageManager": "pnpm@9.13.2",
  "engines": {
    "node": ">=18"
  }
}